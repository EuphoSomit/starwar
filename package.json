{
  "name": "starwar",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "engines": {
    "yarn": "^1.0.0"
  },
  "scripts": {
    "start": "env-cmd -e dev react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "build:dev": "env-cmd -e dev npm run build",
    "build:svt": "env-cmd -e svt npm run build",
    "build:demo": "env-cmd -e demo npm run build",
    "build:prod": "env-cmd -e prod npm run build",
    "test:staged": "cross-env CI=true react-scripts test --env=jsdom --findRelatedTests",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@redux-saga/simple-saga-monitor": "^1.0.2",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "env-cmd": "^9.0.1",
    "font-awesome": "^4.7.0",
    "humps": "^2.0.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "normalizr": "^3.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-form": "^8.2.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.2"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "react-scripts": "^2.1.8",
    "react-test-renderer": "^16.8.6",
    "redux-devtools": "^3.5.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "source-map-explorer": "^1.8.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/index.{js,jsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "lcov",
      "text"
    ]
  }
}
